{
	"name": "@webitel/quality-auditor",
	"version": "25.12.0",
	"private": true,
	"scripts": {
		"dev": "vite --force",
		"build": "vite build",
		"lint:fix": "npx biome check --write ./src",
		"lint:fix-staged": "npx lint-staged",
		"utils:i": "npm install --install-links",
		"utils:link-libs": "npm link @webitel/ui-sdk @webitel/api-services && npm run utils:ls-libs",
		"utils:ls-libs": "npm ls @webitel/ui-sdk @webitel/api-services",
		"utils:up-libs": "npm update @webitel/ui-sdk @webitel/api-services && npm ls @webitel/ui-sdk @webitel/api-services",
		"prepare": "husky || true"
	},
	"type": "module",
	"dependencies": {
		"@floating-ui/vue": "^1.1.6",
		"@morev/vue-transitions": "^3.0.5",
		"@vue/compat": "^3.5.x",
		"@vuelidate/core": "^2.0.3",
		"@vuelidate/validators": "^2.0.4",
		"@webitel/api-services": "^0.1.8",
		"@webitel/styleguide": "~26.2",
		"@webitel/ui-sdk": "~26.2",
		"axios": "^1.13",
		"clipboard-copy": "^4.0.1",
		"deep-equal": "^2.x",
		"deepmerge": "^4.x",
		"lodash-es": "^4.x",
		"pinia": "^3.x",
		"vue": "^3.5.x",
		"vue-i18n": "^11.x",
		"vue-router": "^4.x",
		"vuex": "^4.1.0",
		"webitel-sdk": "^26.1.3"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.11",
		"@vitejs/plugin-vue": "^6.x",
		"@vitest/coverage-v8": "^4.x",
		"@vue/test-utils": "^2.x",
		"globals": "^17.x",
		"husky": "^9.1.7",
		"lint-staged": "^16.x",
		"sass": "^1.78.0",
		"tslib": "^2.8.1",
		"typescript": "5.9.x",
		"vite": "npm:rolldown-vite@^7.3.x",
		"vite-plugin-node-polyfills": "^0.x",
		"vite-plugin-vue-devtools": "^8.x",
		"vitest": "^4.x"
	},
	"optionalDependencies": {
		"@esbuild/darwin-arm64": "^0.24.0",
		"@rollup/rollup-linux-x64-gnu": "^4"
	},
	"overrides": {
		"vite": "npm:rolldown-vite@^7.3.x"
	}
}
